{
  "name": "vue-simple-admin-starter",
  "version": "0.0.1",
  "description": "A basic backend management system template based on Vue3 and pro-components",
  "author": "puppetk-666<https://github.com/puppet-666>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:puppet-666/vue-simple-admin-starter.git"
  },
  "keywords": [
    "vue3",
    "ant-design-vue",
    "ant-design-pro",
    "admin",
    "template-project"
  ],
  "scripts": {
    "dev": "vite",
    "dev:force": "vite --force",
    "build": "vue-tsc --noEmit && vite build",
    "report": "vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.2.2",
    "@ant-design/icons-vue": "^6.1.0",
    "@vueuse/core": "^10.1.2",
    "@vueuse/integrations": "^10.1.2",
    "ant-design-vue": "^3.2.20",
    "axios": "^1.4.0",
    "vue": "^3.2.35",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@chuhoman/eslint-config": "^0.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^17.0.43",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.40.0",
    "less": "^4.1.3",
    "rollup-plugin-visualizer": "^5.8.3",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vue-tsc": "^1.0.24"
  }
}
